    "test:openrouter:minimal": "tsx scripts/test-openrouter-minimal.ts",
    "test:xai": "tsx scripts/test-xai.ts",
    "verify:env": "bash scripts/setup-production-env.sh",
    "prepare:production": "pnpm verify:env && pnpm build:production"
